<% heading 'Settings' %>

<% form_for_resource do |form| %>
  <%= render :partial => 'form', :object => form %>
  <% form.actions do %>
    <%= submit_tag 'Save and continue editing' %>
    <%= submit_tag 'Save' %>
     or <%= cancel_link Space.active ? space_dashboard_url(Space.active) : dashboard_url %>
  <% end %>
<% end %>