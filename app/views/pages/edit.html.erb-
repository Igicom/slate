<% heading 'Editing page' %>

<% form_for_resource do |form| %>
  <%#= form.hidden_field :parent_id %>
  <%= render :partial => 'form', :object => form %>
  <% form.actions do %>
    <%= submit_tag 'Save and continue editing' %> 
    <%= submit_tag 'Save' %>
    or 
    <%= cancel_link %>
  <% end %>  
<% end %>  
  